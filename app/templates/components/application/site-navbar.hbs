<nav id="embernote-navbar" class="navbar navbar-default navbar-fixed-top" role="navigation">
 <div class="container">
   <div class="navbar-header">
       <button class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
           <span class="icon-bar"></span>
           <span class="icon-bar"></span>
           <span class="icon-bar"></span>
       </button>
   </div>
   <div id="navbar" class="collapse navbar-collapse">
           <div class="form-group form-group-md float-lg-middle col-lg-6 col-md-6 col-sm-5 search-form">
               {{input type="text" id="searchInput" class="form-control col-md-12 search-input" placeholder="Search" enter="querySearch"}}
           </div>
           <ul class="nav navbar-nav navbar-right" style="margin-right: 80px;">
               {{#if session.isAuthenticated}}
                   {{#link-to 'user' model.username tagName="li" class="float-lg-right"}}
                       <a id="usernameNav">{{model.username}}</a>
                   {{/link-to}}
                   <li class="float-lg-right"  {{action "signOut"}}>
                       <a class="dropdown-toggle">Log Out</a>
                   </li>
               {{else}}
               <li class="nav-item">
                 <a href="#" id="sign-up-botton" class="nav-link"  data-test-sign-up-header
                   {{action (queue
                     (action (mut authComponent) "sign-in")
                     (toggle "authOpened" this)
                     )}}>
                     Sign-in
                   </a>
              </li>
               {{/if}}
           </ul>
   </div>
 </div>
</nav>
{{! Authentication Modal }}
{{#if authOpened}}
  {{to-elsewhere named="modal" send=(component "application/auth-onboarding"
    modalId="auth-modal"
    component=authComponent
    onClose=(toggle-action "authOpened" this))}}
{{/if}}
